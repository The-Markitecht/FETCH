    00fb : e001 ; -- <0031> br iz :fail
    00fc : 010b ; -- <0031> "
    00fd : 0b10 ; -- <0032> i = ad1
    00fe : 0002 ; -- <0033> a = i
    00ff : fba0 ; -- <0034> call :fetch_byte
    0100 : 00e1 ; -- <0034> "
    0101 : fc00 ; -- <0034> "
    0102 : 0405 ; -- <0035> b = y
    0103 : e407 ; -- <0036> bn eq :again
    0104 : 00fb ; -- <0036> "
