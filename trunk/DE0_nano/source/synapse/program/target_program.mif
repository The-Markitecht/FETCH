    0077 : 2003 ; -- <0005> push j -- "
    0078 : 2004 ; -- <0005> push x -- "
    0079 : 2005 ; -- <0005> push y -- "
    007a : 2006 ; -- <0005> push g6 -- "
    007b : 2007 ; -- <0005> push g7 -- "
-- keeping mo data in g6, mi data in g7.
    007c : 1800 ; -- <0007> g6 = a
    007d : 1e00 ; -- <0008> g7 = 0
-- keeping bit counter in i.
    007e : 0801 ; -- <0010> i = b
    007f : 0f60 ; -- <0011> j = -1
    0080 : 1760 ; -- <0012> y = -1
-- :next_bit -- = 0x0081
-- output the msb of mo, along with a low clock phase and low csn.
    0081 : 0006 ; -- <0015> a = g6
    0082 : 0350 ; -- <0016> a = a>>1
    0083 : 0350 ; -- <0017> a = a>>1
    0084 : 0350 ; -- <0018> a = a>>1
    0085 : 0353 ; -- <0019> a = a>>4
    0086 : 0353 ; -- <0020> a = a>>4
    0087 : 0353 ; -- <0021> a = a>>4
    0088 : 3800 ; -- <0022> de0nano_adc_ctrl = a
-- wait about 500 ns (for about 1 Mhz sck) assuming 50 MHz sysclk.
    0089 : 1206 ; -- <0024> x = 6
-- :wait1 -- = 0x008a
    008a : c800 ; -- <0026> x = x+y
    008b : 1320 ; -- <0026> "
    008c : e402 ; -- <0027> bn xz :wait1
    008d : 008a ; -- <0027> "
-- output a high clock phase.
    008e : 0602 ; -- <0029> b = $de0nano_adc_sck_mask
    008f : c800 ; -- <0030> de0nano_adc_ctrl = or
    0090 : 3b34 ; -- <0030> "
-- sample mi.
    0091 : 0007 ; -- <0032> a = g7
    0092 : 0351 ; -- <0033> a = a<<1
    0093 : 040e ; -- <0034> b = de0nano_adc_ctrl
    0094 : c800 ; -- <0035> g7 = or
    0095 : 1f34 ; -- <0035> "
-- wait about 500 ns (for about 1 Mhz sck) assuming 50 MHz sysclk.
    0096 : 1206 ; -- <0037> x = 6
-- :wait2 -- = 0x0097
    0097 : c800 ; -- <0039> x = x+y
    0098 : 1320 ; -- <0039> "
    0099 : e402 ; -- <0040> bn xz :wait2
    009a : 0097 ; -- <0040> "
