-- :fail -- = 0x010b
    010b : 0360 ; -- <0041> a = -1
    010c : f808 ; -- <0042> pop rtna -- end_func
    010d : 1408 ; -- <0042> pop y -- "
    010e : 0c08 ; -- <0042> pop j -- "
    010f : 0808 ; -- <0042> pop i -- "
    0110 : fc00 ; -- <0042> end_func



