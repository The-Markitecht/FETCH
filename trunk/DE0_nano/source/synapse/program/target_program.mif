-- wait about 500 ns (for about 1 Mhz sck) assuming 50 MHz sysclk.
    00a3 : 1206 ; -- <0051> x = 6
-- :wait3 -- = 0x00a4
    00a4 : c800 ; -- <0053> x = x+y
    00a5 : 1320 ; -- <0053> "
    00a6 : e402 ; -- <0054> bn xz :wait3
    00a7 : 00a4 ; -- <0054> "
