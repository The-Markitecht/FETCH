    00e7 : d300 ; -- <0009> "
    00e8 : 03b0 ; -- <0009> "
    00e9 : 0353 ; -- <0010> a = a>>4
    00ea : 0353 ; -- <0011> a = a>>4
    00eb : fc00 ; -- <0012> rtn
-- :pick_byte_even -- = 0x00ec
    00ec : 0350 ; -- <0014> a = a>>1
    00ed : 0404 ; -- <0015> b = x
    00ee : c800 ; -- <0016> fetch a from a+b
    00ef : d300 ; -- <0016> "
    00f0 : 03b0 ; -- <0016> "
