    00f5 : 2002 ; -- <0027> push i -- func find_in_fetch
    00f6 : 2003 ; -- <0027> push j -- "
    00f7 : 2005 ; -- <0027> push y -- "
    00f8 : 203e ; -- <0027> push rtna -- "
